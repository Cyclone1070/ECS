    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "DesktopLauncher",
            "request": "launch",
            "mainClass": "com.cyclone.projecta.DesktopLauncher",
            "projectName": "projectA-desktop",
            "vmArgs": "-XstartOnFirstThread"
        },
        {
            "type": "java",
            "name": "IOSLauncher",
            "request": "launch",
            "mainClass": "com.cyclone.projecta.IOSLauncher",
            "projectName": "projectA-ios"
        }
    ]
}